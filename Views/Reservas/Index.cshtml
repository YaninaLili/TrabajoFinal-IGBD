@model List<Reserva>

<h1>Lista de reservas</h1>

@if (Model.Count == 0) {
    <p>No hay reservas registradas</p>
}

<ul class="list">
@foreach (var item in Model)
{
    <li>@item.DNI</li>
    <li>@item.Nombre</li>
    <li>@item.Apellido</li>
    <li>@item.Correo</li>
    <li>@item.Celular</li>
    <li>@item.Habitacion</li>
}
</ul>